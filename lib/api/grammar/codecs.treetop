grammar Codecs

  rule video
    ( [xhH] '264' /
      [xX] [vV] [iI] [dD] /
      [dD] [iI] [vV] [xX] /
      [mM] [pP] [eE] [gG] [2]) {
        def content
          [:video, text_value.downcase]
        end
      }
  end

  rule audio
    ( [fF] [lL] [aA] [cC] /
      [aA] [aA] [cC] /
      [oO] [gG] [gG] /
      [vV] [oO] [rR] [bB] [iI] [sS]) {
        def content
          [:audio, text_value.downcase]
        end
      }
  end

end
